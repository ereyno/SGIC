@model SGIC.ViewModel.PersonaViewModel

<script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)
    <fieldset>
        <legend>Automovil</legend>

        <div class="editor-label">
            @Html.LabelFor(model => model.nuevoAutomovil.patente)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.nuevoAutomovil.patente)
            @Html.ValidationMessageFor(model => model.nuevoAutomovil.patente)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.nuevoAutomovil.fechaInicio)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.nuevoAutomovil.fechaInicio)
            @Html.ValidationMessageFor(model => model.nuevoAutomovil.fechaInicio)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.nuevoAutomovil.foto)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.nuevoAutomovil.foto)
            @Html.ValidationMessageFor(model => model.nuevoAutomovil.foto)
        </div>

        <p>
            <input type="submit" value="Create" />
        </p>
    </fieldset>
}
